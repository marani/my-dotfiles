[user]
	name = marani
	email = dung1@e.ntu.edu.sg
[mergetool]
	keepBackup = false

[alias]
	br = branch
	g = log --pretty=oneline --graph --abbrev-commit
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	st = status
	ci = commit
	co = checkout
	graph = log --graph --full-history --all --color --pretty=format:"%x1b[31m%h%x09%x1b[32m%d%x1b[0m%x20%s"
graph2 = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
	diff = diff -w

[color]
	branch = auto
	diff = auto
	status = auto

[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[push]
	default = simple
